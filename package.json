    "test-both": "npm run test-php && npm run test"
  },
  "dependencies": {
    "perhaps-ts": "^0.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "ts-node": "^10.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "1.6.0",
    "vite-plugin-dts": "^3.8.3"
