{
  "name": "@lnear/cache",
  "description": "Lightweight LRU and LFU cache implementations for Node.js and the browser.",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "lru",
    "cache",
    "lnear"
  ],
  "homepage": "https://docs.lnear.dev/cache",
  "author": {
    "name": "lnear",
    "email": "hi@lnear.dev",
    "url": "https://lnear.dev"
  },
  "maintainers": [
    {
      "name": "lnear",
      "email": "hi@lnear.dev",
      "url": "https://lnear.dev"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lnear-dev/cache.git"
  },
  "main": "./dist/lnear-cache.js",
  "typings": "./dist/lnear-cache.d.ts",
  "module": "./dist/lnear-cache.js",
